{
  "name": "gutenberg-blocks",
  "version": "1.0.0",
  "description": "Custom 3rd party gutenberg blocks",
  "main": "index.js",
  "scripts": {
    "b-tweet": "babel --plugins transform-react-jsx blocks/tweet/block.js -o blocks/tweet/block-build.js --minified",
